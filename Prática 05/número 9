import java.util.Scanner;
public class NumerosImparesDe1Informado {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Informe um n√∫mero:");
        n = scanner.nextInt();
        for (int i = 1; i <= n; i++) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
        }    
    }
}