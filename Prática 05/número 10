import java.util.Scanner;
public class QuantasMulheresTemEntre20e40Anos {
    public static void main(String[] args);
    Scanner scanner = new Scanner(System.in);
    int countMulheres = 0;
    for (int i = 1; i <= 10; i++) {
        System.out.println("Informe a idade e o sexo (M/F) da pessoa " + i + ":");
        idade = scanner.nextInt();
        char sexo = scanner.next().toUpperCase().charAt(0);
        if (sexo == 'F' && idade >= 20 && idade <= 40) {
            countMulheres++;
        }
    }
    System.out.println("Mulheres entre 20 e 40 anos: " + countMulheres);
}