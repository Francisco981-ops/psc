Import java.util.*
public class CalcularIdade {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Digite o ano de nascismento: 2005");
        int anoNascimento = input.nextInt();
        System.out.println("Digite o ano atual: 2024");
        int anoAtual = input.nextInt();
        int idadeAtual = anoAtual - anoNascimento;
        int idadeEm2050 = 2050 - anoNascimento;
        System.out.println("A idade atual é: 19" + idadeAtual);
        System.out.println("A idade em 2050 será: 45 " + idadeEm2050);
    }
}