Import java.util.*
public class SalariosMinimos {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Digite o valor do salário mínimo: ");
        double salariosMinimo = input.nextDouble();
        System.out.println("Digite o valor do salário do funcionário:  ");
        double salarioFuncionario = input.nextDouble();
        double quantidadeSalarios = salarioFuncionario / salariosMinimo;
        System.out.println("O funcionario recebe " + quantidadeSalarios + "salários mínimos.");
    }
}